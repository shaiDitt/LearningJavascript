// Somar os primeros 10 numeros multiplos de 3 e 5 

somarNumeros();

function somarNumeros(){
  let multiplosDe3=0
  let multiplosDe5=0
  for(let i = 0; i < 11; i++ ) {
    if (i%3===0){
      multiplosDe3 += i;
    }
      if (i%5===0){
      multiplosDe5 += i;      
      }
  }
  console.log(multiplosDe3+multiplosDe5);
}
